target 'Farm To Fork' do
  use_frameworks!
  platform :ios, '11.0'
  pod 'MaterialTextField'
  pod 'DeviceKit'
  pod 'SCLAlertView'
  pod 'Valet', :git => 'https://github.com/square/Valet.git', :branch => 'dfed/watchOS'
end

target 'Farm To Fork For Apple Watch Extension' do
    use_frameworks!
    platform :watchos, '4.0'
    pod 'Valet', :git => 'https://github.com/square/Valet.git', :branch => 'dfed/watchOS'
end
